@model Person

@{
    ViewData["Title"] = "Create a person";
}

<h1>@ViewData["Title"]</h1>


<form method="post">
    <partial name="CreateEditPartial" model="Model"/>

    <button type="submit" class="btn btn-primary">Create</button>
</form>

@section Scripts
{
    <partial name="_ValidationScriptsPartial" />
}
